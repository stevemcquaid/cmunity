<h1>Listing url_posts</h1>

<table>
  <tr>
    <th>Content</th>
    <th>Url</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @url_posts.each do |url_post| %>
  <tr>
    <td><%= url_post.content_id %></td>
    <td><%= url_post.url %></td>
    <td><%= link_to 'Show', url_post %></td>
    <td><%= link_to 'Edit', edit_url_post_path(url_post) %></td>
    <td><%= link_to 'Destroy', url_post, confirm: 'Are you sure?', method: :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Url post', new_url_post_path %>
