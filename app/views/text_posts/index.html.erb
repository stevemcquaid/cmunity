<h1>Listing text_posts</h1>

<table>
  <tr>
    <th>Content</th>
    <th>Text</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @text_posts.each do |text_post| %>
  <tr>
    <td><%= text_post.content_id %></td>
    <td><%= text_post.text %></td>
    <td><%= link_to 'Show', text_post %></td>
    <td><%= link_to 'Edit', edit_text_post_path(text_post) %></td>
    <td><%= link_to 'Destroy', text_post, confirm: 'Are you sure?', method: :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Text post', new_text_post_path %>
