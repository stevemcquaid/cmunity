<h1>Listing event_posts</h1>

<table>
  <tr>
    <th>Content</th>
    <th>Location</th>
    <th>Description</th>
    <th>Start datetime</th>
    <th>End datetime</th>
    <th>Is all day</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @event_posts.each do |event_post| %>
  <tr>
    <td><%= event_post.content_id %></td>
    <td><%= event_post.location %></td>
    <td><%= event_post.description %></td>
    <td><%= event_post.start_datetime %></td>
    <td><%= event_post.end_datetime %></td>
    <td><%= event_post.is_all_day %></td>
    <td><%= link_to 'Show', event_post %></td>
    <td><%= link_to 'Edit', edit_event_post_path(event_post) %></td>
    <td><%= link_to 'Destroy', event_post, confirm: 'Are you sure?', method: :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Event post', new_event_post_path %>
